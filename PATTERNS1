   int n;
cout<<"enter no.";
  
  cin>>n;

   int l=1;
  for(int i=1;i<=n;i++){
    
    for(int k=1;k<=n-i;k++){
      cout<<" ";
    }
    
    for(int j=0;j<=i-1;j++){
      cout<<l;
      
    }
    
    
    
     
       
